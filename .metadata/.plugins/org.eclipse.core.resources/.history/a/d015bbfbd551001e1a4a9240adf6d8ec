
public class MathClass {

	public static void main(String[] args) {
		
		double x = 3.14;
		double y = -10;
		double w = 25;
		
		double z = Math.max(x, y);
		double m = Math.min(x, y);
		double a = Math.abs(y);
		double s = Math.sqrt(w);
		

		
		
		System.out.println(z);
		System.out.println(m);
		System.out.println(a);
		System.out.println(s);



	}

}
